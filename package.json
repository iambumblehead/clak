{
  "name": "clak",
  "type": "module",
  "version": "0.0.1",
  "license": "ISC",
  "readmeFilename": "README.md",
  "description": "returns localised values from google-formatted csv translation file",
  "author": "chris <chris@bumblehead.com>",
  "main": "clak.js",
  "browser": "clak.js",
  "module": "clak.mjs",
  "exports": {
    "require": "./clak.js",
    "import": "./clak.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iambumblehead/clak.git"
  },
  "files": [
    "clak.js",
    "clak.mjs",
    "README.md"
  ],
  "keywords": [
    "internationalization",
    "localization",
    "translate",
    "csv",
    "unity",
    "excel",
    "i18n"
  ],
  "devDependencies": {
    "c8": "^7.12.0",
    "eslint": "^8.23.1",
    "eslint-plugin-markdown": "^3.0.0"
  },
  "scripts": {
    "test": "npm start && node --test",
    "lint": "eslint clak.mjs",
    "start": "cp clak.mjs clak.js && sed -i 's/export default/module.exports =/' clak.js",
    "prepublishOnly": "npm start && npm run lint && npm test"
  }
}
